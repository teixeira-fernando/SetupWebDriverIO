{
  "name": "E2E",
  "version": "1.0.0",
  "description": "", 
  "main": "index.js",
  "scripts": {
    "test:local": "./node_modules/.bin/wdio wdio.conf.js",
    "test:zalenium": "./node_modules/.bin/wdio wdio.conf.zalenium.js",
    "test:withZaleniumSetup": "npm run --silent zalenium:setup && docker wait zalenium && ./node_modules/.bin/wdio wdio.conf.zalenium.js",
    "zalenium:setup": "docker run -dit --rm -ti --name zalenium -p 4444:4444 -v /var/run/docker.sock:/var/run/docker.sock -v /tmp/videos:/home/seluser/videos --privileged dosel/zalenium start --maxTestSessions 6 --videoRecordingEnabled false",
    "zalenium:down": "docker stop zalenium",
    "docker:up:grid": "docker-compose up -d",
    "docker:down:grid": "docker-compose down",
    "allure-report": "node_modules/.bin/allure generate ./test/reports/allure-results -c && allure open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/webdriverio": "^5.0.0",
    "@wdio/allure-reporter": "^5.11.0",
    "@wdio/cli": "^5.11.5",
    "@wdio/cucumber-framework": "^5.11.3",
    "@wdio/junit-reporter": "^5.11.0",
    "@wdio/local-runner": "^5.11.5",
    "@wdio/selenium-standalone-service": "^5.11.2",
    "@wdio/spec-reporter": "^5.11.0",
    "@wdio/sync": "^5.11.0",
    "chai": "^4.2.0",
    "wdio-selenium-standalone-service": "0.0.12",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^5.11.5"
  },
  "dependencies": {}
}
